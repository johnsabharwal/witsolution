<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Wits-Solution</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <!-- Google Web Fonts -->
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Nunito:wght@400;600;700;800&family=Rubik:wght@400;500;600;700&display=swap" rel="stylesheet">
    <link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.10.0/css/all.min.css" rel="stylesheet">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.4.1/font/bootstrap-icons.css" rel="stylesheet">
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/WebApplication1.styles.css" asp-append-version="true" />
    <link href="~/lib/owlcarousel/assets/owl.carousel.min.css" rel="stylesheet">
    <link href="~/lib/animate/animate.min.css" rel="stylesheet">
    <link rel="stylesheet" href="//cdnjs.cloudflare.com/ajax/libs/toastr.js/latest/css/toastr.min.css" />
  
</head>
<body>
      <header>
   
        <div id="spinner" class="show bg-white position-fixed translate-middle w-100 vh-100 top-50 start-50 d-flex align-items-center justify-content-center">
            <div class="spinner"></div>
        </div>
        <!-- Spinner End -->
        <!-- Topbar Start -->
        <div class="container-fluid bg-dark px-5 d-none d-lg-block">
            <div class="row gx-0">
                <div class="col-lg-8 text-center text-lg-start mb-2 mb-lg-0">
                    <div class="d-inline-flex align-items-center" style="height: 45px;">
                        <small class="me-3 text-light"><i class="fa fa-map-marker-alt me-2"></i>Amritsar, Punjab</small>
                        <small class="me-3 text-light"><i class="fa fa-phone-alt me-2"></i>@ConstContent.Number</small>
                        <small class="text-light"><i class="fa fa-envelope-open me-2"></i>@ConstContent.MyEmail</small>
                    </div>
                </div>
                <div class="col-lg-4 text-center text-lg-end">
                    <div class="d-inline-flex align-items-center" style="height: 45px;">
                        <a class="btn btn-sm btn-outline-light btn-sm-square rounded-circle me-2" href=""><i class="fab fa-twitter fw-normal"></i></a>
                        <a class="btn btn-sm btn-outline-light btn-sm-square rounded-circle me-2" href=""><i class="fab fa-facebook-f fw-normal"></i></a>
                        <a class="btn btn-sm btn-outline-light btn-sm-square rounded-circle me-2" href=""><i class="fab fa-linkedin-in fw-normal"></i></a>
                        <a class="btn btn-sm btn-outline-light btn-sm-square rounded-circle me-2" href=""><i class="fab fa-instagram fw-normal"></i></a>
                        @* <a class="btn btn-sm btn-outline-light btn-sm-square rounded-circle" href=""><i class="fab fa-youtube fw-normal"></i></a> *@
                    </div>
                </div>
            </div>
        </div>
        <!-- Topbar End -->
        <!-- Navbar & Carousel Start -->
        <div class="container-fluid  position-relative p-0">
            <nav class="navbar navbar-toggler navbar-expand-lg navbar-dark px-5 py-3 py-lg-0">
                <a href="/" class="navbar-brand p-0">
                    <h1 class="m-0 text-dark">Wit-Solutions</h1>
                </a>
                <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarCollapse">
                    <span class="fa fa-bars"></span>
                </button>
                <div class="collapse navbar-collapse" id="navbarCollapse">
                    <div class="navbar-nav  ms-auto py-0">
                        <a asp-controller="Home" asp-action="Index" class="nav-item text-dark nav-link @(ViewBag.Class=="Home"?"active":"")">Home</a>
                        <div class="nav-item dropdown ">
                            <a class="nav-link text-dark dropdown-toggle @(ViewBag.AboutDropDown==true?"active":"")" data-bs-toggle="dropdown">About</a>
                            <div class="dropdown-menu m-0">
                                <a asp-controller="About" asp-action="Index" class="dropdown-item @(ViewBag.Class=="About"?"active":"")">About Company</a>
                                <a asp-controller="About" asp-action="Vision" class="dropdown-item  @(ViewBag.Class=="Vision"?"active":"")">Vision</a>
                            </div>
                        </div>
                        <div class="nav-item dropdown">
                            <a class="nav-link text-dark dropdown-toggle @(ViewBag.ServicesDropDown==true?"active":"")" data-bs-toggle="dropdown">Services</a>
                            <div class="dropdown-menu m-0">
                                <a asp-controller="Service" asp-action="Index" class="dropdown-item @(ViewBag.Class=="Software"?"active":"")">Software Application Development</a>
                                <a asp-controller="Service" asp-action="DataAnatytics" class="dropdown-item @(ViewBag.Class=="Data"?"active":"")">Data Analytics</a>
                                <a asp-controller="Service" asp-action="Devops" class="dropdown-item @(ViewBag.Class=="DevOps"?"active":"")">DevOps</a>
                                <a asp-controller="Service" asp-action="SoftwareTesting" class="dropdown-item @(ViewBag.Class=="Testing"?"active":"")">Software Testing</a>
                                <a asp-controller="Service" asp-action="HybridMobileApp" class="dropdown-item @(ViewBag.Class=="Hybrid"?"active":"")">Hybrid Mobile Development</a>
                                <a asp-controller="Service" asp-action="GraphicDesign" class="dropdown-item @(ViewBag.Class=="Graphic"?"active":"")">Graphic Designing</a>
                                <a asp-controller="Service" asp-action="SearchEngineOpt" class="dropdown-item @(ViewBag.Class=="Search"?"active":"")">Search Engine Optimization</a>
                                <a asp-controller="Service" asp-action="Ecommerce" class="dropdown-item @(ViewBag.Class=="Ecommerce"?"active":"")">Ecommerce Optimization</a>
                                <a asp-controller="Service" asp-action="DigitalMarketing" class="dropdown-item @(ViewBag.Class=="Digital"?"active":"")">Digital Marketing Specialization</a>
                                <a asp-controller="Service" asp-action="SocialMediaMarketing" class="dropdown-item @(ViewBag.Class=="Social"?"active":"")">Social Media Marketing</a>
                            </div>
                        </div>
                       
                        <div class="nav-item dropdown">
                            <a href="#" class="nav-link text-dark dropdown-toggle @(ViewBag.TechnologyDropDown==true?"active":"")" data-bs-toggle="dropdown">Technologies</a>
                            <div class="dropdown-menu m-0">
                                <a asp-controller="Technologies" asp-action="Index" class="dropdown-item @(ViewBag.Class=="Net"?"active":"") ">.Net</a>
                              
                                <a asp-controller="Technologies" asp-action="Php" class="dropdown-item @(ViewBag.Class=="Php"?"active":"")">PHP</a>
                                <a asp-controller="Technologies" asp-action="Cloud" class="dropdown-item @(ViewBag.Class=="Cloud"?"active":"")">Cloud Computing</a>
                            </div>
                        </div>
                        <a asp-controller="Contact" asp-action="Index" class="nav-item text-dark nav-link @(ViewBag.Class=="Contact"?"active":"")">Contact</a>
                    </div>
                   @*  <butaton type="button" class="btn text-primary ms-3" data-bs-toggle="modal" data-bs-target="#searchModal"><i class="fa fa-search"></i></butaton> *@
                  
                </div>
            </nav>

           
        </div> 
        <!-- Navbar & Carousel End -->
   </header>
    
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
            <br /><br />
        </main>
    </div>

    <footer class="border-top footer  text-muted absolute w-100 white-space no-wrap">
        <!-- Footer Start -->
        <div class="container-fluid bg-dark text-light mt-5 wow fadeInUp" data-wow-delay="0.1s">
            <div class="container">
                <div class="row gx-5">
                    <div class="col-lg-4 col-md-6 footer-about">
                        <div class="d-flex flex-column align-items-center justify-content-center text-center h-100 bg-primary p-4">
                            <a asp-controller="Home" asp-action="Index" class="navbar-brand">
                                <h1 class="m-0 text-white">Wits Solutions</h1>
                            </a>
                            <p class="mt-3 mb-4"></p>
                            
                        </div>
                    </div>
                    <div class="col-lg-8 col-md-6">
                        <div class="row gx-5">
                            <div class="col-lg-4 col-md-12 pt-5 mb-5">
                                <div class="section-title section-title-sm position-relative pb-3 mb-4">
                                    <h3 class="text-light mb-0">Get In Touch</h3>
                                </div>
                                <div class="d-flex mb-2">
                                    <i class="bi bi-geo-alt text-primary me-2"></i>
                                    <p class="mb-0">Amritsar,Punjab</p>
                                </div>
                                <div class="d-flex mb-2">
                                    <i class="bi bi-envelope-open text-primary me-2"></i>
                                    <p class="mb-0">@ConstContent.MyEmail</p>
                                </div>
                                <div class="d-flex mb-2">
                                    <i class="bi bi-telephone text-primary me-2"></i>
                                    <p class="mb-0">@ConstContent.Number</p>
                                </div>
                                <div class="d-flex mt-4">
                                    <a class="btn btn-primary btn-square me-2" href="#"><i class="fab fa-twitter fw-normal"></i></a>
                                    <a class="btn btn-primary btn-square me-2" href="#"><i class="fab fa-facebook-f fw-normal"></i></a>
                                    <a class="btn btn-primary btn-square me-2" href="#"><i class="fab fa-linkedin-in fw-normal"></i></a>
                                    <a class="btn btn-primary btn-square" href="#"><i class="fab fa-instagram fw-normal"></i></a>
                                </div>
                            </div>
                            <div class="col-lg-4 col-md-12 pt-0 pt-lg-5 mb-5">
                                <div class="section-title section-title-sm position-relative pb-3 mb-4">
                                    <h3 class="text-light mb-0">Quick Links</h3>
                                </div>
                                <div class="link-animated d-flex flex-column justify-content-start">
                                    <a class="text-light mb-2" asp-controller="Home" asp-action="Index"><i class="bi bi-arrow-right text-primary me-2"></i>Home</a>
                                    <a class="text-light mb-2" asp-controller="About" asp-action="Index"><i class="bi bi-arrow-right text-primary me-2"></i>About Us</a>
                                    <a class="text-light mb-2" asp-Controller="Service" asp-action="Index"><i class="bi bi-arrow-right text-primary me-2"></i>Our Services</a>
                                  
                                    <a class="text-light" asp-controller="Contact" asp-action="Index" ><i class="bi bi-arrow-right text-primary me-2"></i>Contact Us</a>
                                </div>
                            </div>
                            <div class="col-lg-4 col-md-12 pt-0 pt-lg-5 mb-5">
                                <div class="section-title section-title-sm position-relative pb-3 mb-4">
                                    <h3 class="text-light mb-0">Popular Links</h3>
                                </div>
                                <div class="link-animated d-flex flex-column justify-content-start">
                                    <a class="text-light mb-2" asp-controller="Home" asp-action="Index"><i class="bi bi-arrow-right text-primary me-2"></i>Home</a>
                                    <a class="text-light mb-2" asp-controller="About" asp-action="Index"><i class="bi bi-arrow-right text-primary me-2"></i>About Us</a>
                                    <a class="text-light mb-2" asp-controller="Service" asp-action="Index"><i class="bi bi-arrow-right text-primary me-2"></i>Our Services</a>
                                    
                                    <a class="text-light" asp-controller="Contact" asp-action="Index"><i class="bi bi-arrow-right text-primary me-2"></i>Contact Us</a>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="container-fluid text-white" style="background: #061429;">
            <div class="container text-center">
                <div class="row justify-content-end">
                    <div class="col-lg-8 col-md-6">
                        <div class="d-flex align-items-center justify-content-center" style="height: 75px;">
                            <p class="mb-0">
                                &copy; <a class="text-white border-bottom" href="#">Wits-Solution</a>. All Rights Reserved.

                                <!--/*** This template is free as long as you keep the footer author’s credit link/attribution link/backlink. If you'd like to use the template without the footer author’s credit link/attribution link/backlink, you can purchase the Credit Removal License from "https://htmlcodex.com/credit-removal". Thank you for your support. ***/-->
                                Designed by <a class="text-white border-bottom" href="">us</a>
                            </p>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- Footer End -->
    </footer>
    <!-- Back to Top -->
        
       
        <div class="whatsapp">
        <a href=" https://wa.me/9803482335">
                <img src="~/img/whatsappicon.jpg" width="50px"  alt="" />
            </a>
        </div>
    
       
    
    
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    <script src="~/lib/wow/wow.js"></script>
    <script src="~/lib/easing/easing.min.js"></script>
    <script src="~/lib/waypoints/waypoints.min.js"></script>
    <script src="~/lib/counterup/counterup.min.js"></script>
    <script src="~/lib/owlcarousel/owl.carousel.js"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
